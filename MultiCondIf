public class MultiCondIf 
{

	public static void main(String[] args) 
	{
	int a=100, b=80, c=90, d=400;
		
		if (a>b)
		{
			if (a>c)
			{
				if(a>d)
				{
					System.out.println("A is a big number");
				}
				else 
					// pertains to third condition i.e. (a>d)
				{
					System.out.println("A is not a big number");
				}
			}
			else 
				// pertains to second condition i.e. (a>c)
				
			{
				System.out.println("A is not a big number");
			}
		}
			
		else 
			// pertains to first condition i.e. (a>b)
		{
			System.out.println("A is not a big number");
		}
	}
}
